		switch(buttonID) {
			case "upBlood":
				//bloodToken++;//This will be a vraiable
				//updateText("boxes", boxes);//updateText("boxes", boxes); update the blood damage token here.
				break;
			case "downBlood":
				//bloodToken--;//This will be a vraiable
				//updateText("boxes", boxes); update the blood damage token here.
				break;
		}
